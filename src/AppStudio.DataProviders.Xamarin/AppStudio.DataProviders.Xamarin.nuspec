<?xml version="1.0"?>
<package >
  <metadata>
    <id>WindowsAppStudio.DataProviders.Xamarin</id>
    <version>$version$</version>
    <title>Windows AppStudio Data Providers for Xamarin (non-official)</title>
    <authors>Alchiweb</authors>
    <owners>Alchiweb</owners>
    <licenseUrl>https://raw.githubusercontent.com/mugstrasbourg/waslibs.Xamarin/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/mugstrasbourg/waslibs.Xamarin</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Implementation of the data sources provided by Windows AppStudio. This library enable access to different data source like RSS, Bing, Facebook, Flickr, Instagram, Twitter and YouTube.</description>
